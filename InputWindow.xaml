<Window x:Class="ChordSheetConverter.InputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="InputWindow" Height="400" Width="300">
    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <StackPanel Name="InputStackPanel" Margin="10"/>
        </ScrollViewer>
        <Button Content="Save" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="10" Click="save_Click"/>
    </Grid>
</Window>
